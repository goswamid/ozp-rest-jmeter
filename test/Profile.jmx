              <stringProp name="filename"></stringProp>
              <stringProp name="script"> // Turn the JSON into an object called &apos;JSonResponse&apos;
eval(&apos;var response = &apos; + prev.getResponseDataAsString());
var c = 0;

while (c &lt; response.total){
	var username = response._embedded.item[c].username;
	if (username === &quot;testAdmin1&quot;){
		var profileLink = 	response._embedded.item[c]._links.self.href;
		c = response.total;
	}
	c++;
	}
	
var res = profileLink.split(&apos;/&apos;);
//OUT.println(res[6]);
vars.put(&quot;profileID&quot;,res[6]);</stringProp>
